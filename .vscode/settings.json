{
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).js, $(capture).*.ts",
    "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
    "*.jsx": "$(capture).js",
    "*.tsx": "$(capture).ts",
    "package.json": ".*, *.md, *.config.js, package-lock.json, package.json, LICENSE, components.json, jsconfig.json"
  },
  "cSpell.words": [
    "clsx",
    "colorjs",
    "gtag",
    "onfigurable",
    "svgr",
    "WCAG",
    "коммитах",
    "Коммиты",
    "форк"
  ],
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ]
}
